    "#Feb 15,2024\n",
    "def largestPerimeter(nums):\n",
    "    nums.sort()\n",
    "    n=len(nums)\n",
    "    sum1=sum(nums)\n",
    "    for i in range(n-1,1,-1):\n",
    "        sum1-=nums[i]\n",
    "        if sum1>nums[i]:\n",
    "            return sum1+nums[i]\n",
    "    return -1\n",
    "print(largestPerimeter(list(map(int,input().split()))))"
   ]
  }
